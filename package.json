{
  "name": "finfactory",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "prettier": "cross-env NODE_OPTIONS=--no-deprecation prettier \"**/*.{ts,tsx,js,css,html,md,mdx}\" --write",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "pm2:start": "pm2 restart finfactory-website || pm2 start npm --name 'finfactory-website' -- start"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.5.1",
    "@chakra-ui/system": "^2.6.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@payloadcms/db-mongodb": "^3.42.0",
    "@payloadcms/email-nodemailer": "^3.42.0",
    "@payloadcms/live-preview-react": "^3.42.0",
    "@payloadcms/next": "^3.42.0",
    "@payloadcms/plugin-seo": "^3.42.0",
    "@payloadcms/richtext-lexical": "^3.42.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.0.5",
    "graphql": "^16.8.1",
    "next": "15.1.5",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.10.0",
    "payload": "3.42.0",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "swiper": "^11.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
